{
  "schema": {
    "Meeting": {
      "included": {
        "var_const": "var",
        "type": "binary"
      },
      "time_start": {
        "var_const": "var",
        "type": "int",
        "range_low": 0,
        "range_high": 24
      },
      "time_start_lower_boundary": {
        "var_const": "const"
      },
      "time_start_upper_boundary": {
        "var_const": "const"
      },
      "duration": {
        "var_const": "const"
      },
      "potential_revenue": {
        "var_const": "const"
      }
    }
  },
  "input_method": "json",
  "output_method": "file",
  "stopping_criteria": "convergence",
  "optimizer_settings": {
    "num_generations": 500,
    "num_parents_mating": 4,
    "parent_selection_type": "sss",
    "keep_parents": 1,
    "crossover_type": "single_point",
    "mutation_type": "random",
    "mutation_percent_genes": 40,
    "parallel_method": "thread"
  }
}